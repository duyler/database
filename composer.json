{
    "name": "duyler/database",
    "type": "library",
    "description": "Duyler database library",
    "license": "MIT",
    "keywords": [
        "database",
        "db",
        "orm"
    ],
    "authors": [
        {
            "name": "Mikhail Ilinsky",
            "email": "mail@milinsky.ru"
        }
    ],
    "support": {
        "docs": "https://duyler.org/docs/database/"
    },
    "require": {
        "php": "^8.3",
        "duyler/framework": "dev-main",
        "doctrine/orm": "^3.2",
        "symfony/cache": "^7.0",
        "doctrine/migrations": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.30",
        "squizlabs/php_codesniffer": "^4.0",
        "vimeo/psalm": "^5.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Duyler\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duyler\\Database\\Test\\": ["tests/"]
        }
    }
}
